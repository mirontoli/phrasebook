{
  "phrases": [
    {
      "id": 1,
      "text": {
        "cv": "Эпӗ хирӗҫ мар.",
        "ru": "Я не против."
      }
    },
    {
      "id": 2,
      "text": {
        "cv": "Вӑл ухмах мар.",
        "ru": "Он не дурак."
      }
    },
    {
      "id": 3,
      "text": {
        "cv": "Эпир печчӗн мар.",
        "ru": "Мы не одни."
      }
    },
    {
      "id": 4,
      "text": {
        "cv": "Шуҫса кайса ӳкрӗм.",
        "ru": "Я поскользнулся и упал."
      }
    },
    {
      "id": 5,
      "text": {
        "cv": "Аван чупатӑр-и?",
        "ru": "Как поживаете?"
      }
    },
    {
      "id": 6,
      "text": {
        "cv": "Ай, аван-ҫке!",
        "ru": "Ой, как хорошо!"
      }
    },
    {
      "id": 7,
      "text": {
        "cv": "Эсӗ пӗчченех килтӗн-и?",
        "ru": "Ты пришел один?"
      }
    },
    {
      "id": 8,
      "text": {
        "cv": "Йӳнӗ ларчӗ.",
        "ru": "Мне это обошлось дешево."
      }
    },
    {
      "id": 9,
      "text": {
        "cv": "Апата кил!",
        "ru": "Садись есть"
      }
    },
    {
      "id": 10,
      "text": {
        "cv": "Санӑн ача-пӑча пур-и?",
        "ru": "У тебя есть дети?"
      }
    }
  ]
}