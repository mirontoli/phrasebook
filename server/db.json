[{"id":"1","text":{"cv":"Ав мӗнле иккен!","ru":"Вон оно как!"}},{"id":"2","text":{"cv":"Аван вӗренет.","ru":"Он хорошо учится."}},{"id":"3","text":{"cv":"Аван чупатӑр-и?","ru":"Как поживаете?"}},{"id":"4","text":{"cv":"Аван, эп хатӗр.","ru":"Хорошо, я готов."}},{"id":"5","text":{"cv":"Ай чипер те-ҫке хӗрӗ!","ru":"Ну и красивая же девушка!"}},{"id":"6","text":{"cv":"Ай, аван-ҫке!","ru":"Ой, как хорошо!"}},{"id":"7","text":{"cv":"Ак тамаша!","ru":"Вот невезение!"}},{"id":"8","text":{"cv":"Ан калаҫ!","ru":"Что ты говоришь!"}},{"id":"9","text":{"cv":"Ан кансӗрле!","ru":"Не мешай!"}},{"id":"10","text":{"cv":"Ан кӳрен.","ru":"Не обижайся."}},{"id":"11","text":{"cv":"Ан ҫиллен.","ru":"Не сердись."}},{"id":"12","text":{"cv":"Ан та кала!","ru":"И не говори!"}},{"id":"13","text":{"cv":"Ан тӗкӗн!","ru":"Не трогай!"}},{"id":"14","text":{"cv":"Ан чӗн!","ru":"Не возражай!"}},{"id":"15","text":{"cv":"Ӑнӑҫу пултӑр!","ru":"Желаю удачи!"}},{"id":"16","text":{"cv":"Ӑнланмалла.","ru":"Понятно."}},{"id":"17","text":{"cv":"Апата кил!","ru":"Садись есть!"}},{"id":"18","text":{"cv":"Апачӗ тутлӑ пултӑр!","ru":"Приятного аппетита!"}},{"id":"19","text":{"cv":"Аса илейместӗп.","ru":"Не могу вспомнить."}},{"id":"20","text":{"cv":"Асӑрханӑр!","ru":"Осторожно!"}},{"id":"21","text":{"cv":"Ачасем выляса йӑлӑхрӗҫ.","ru":"Детям надоело играть."}},{"id":"22","text":{"cv":"Вӑл ухмах мар.","ru":"Он не дурак."}},{"id":"23","text":{"cv":"Вӑхӑт хӑвӑрт иртет.","ru":"Время идет быстро."}},{"id":"24","text":{"cv":"Вӗсем пӗр-пӗрне кӑмӑллаҫҫӗ.","ru":"Они любят друг друга."}},{"id":"25","text":{"cv":"Вуласа йӑлӑхтарчӗ.","ru":"Мне надоело читать."}},{"id":"26","text":{"cv":"Ирхине ман апат анмасть.","ru":"Утром у меня нет аппетита."}},{"id":"27","text":{"cv":"Йӳне ларчӗ.","ru":"Мне это обошлось дешево"}},{"id":"28","text":{"cv":"Ку ман упӑшка.","ru":"Это мой муж"}},{"id":"29","text":{"cv":"Ку питӗ йӳнӗ.","ru":"Это очень дешево."}},{"id":"30","text":{"cv":"Ку питӗ хаклӑ.","ru":"Это очень дорого."}},{"id":"31","text":{"cv":"Кунӗпе ҫумӑр ҫӑвать.","ru":"Целый день идет дождь."}},{"id":"32","text":{"cv":"Кунта питӗ илемлӗ.","ru":"Здесь очень красиво."}},{"id":"33","text":{"cv":"Курманни нумай пулать.","ru":"Давно не виделись."}},{"id":"34","text":{"cv":"Маншӑн пурпӗрех.","ru":"Мне все равно."}},{"id":"35","text":{"cv":"Мӗн пулнӑ?","ru":"Что случилось?"}},{"id":"36","text":{"cv":"Миҫе сехетре выртатӑр?","ru":"Во сколько вы ложитесь спать?"}},{"id":"37","text":{"cv":"Паян мӗн кун?","ru":"Какой сегодня день?"}},{"id":"38","text":{"cv":"Паян ҫанталӑк аван.","ru":"Сегодня хорошая погода."}},{"id":"39","text":{"cv":"Санӑн ача-пӑча пур-и?","ru":"У тебя есть дети?"}},{"id":"40","text":{"cv":"Сире мӗнле чӗнеҫҫӗ?","ru":"Как вас зовут?"}},{"id":"41","text":{"cv":"Тем мар.","ru":"Не за что."}},{"id":"42","text":{"cv":"Тӗп-тӗрӗс.","ru":"Совершенно верно."}},{"id":"43","text":{"cv":"Чӑнах-и?","ru":"Неужели? Правда?"}},{"id":"44","text":{"cv":"Шуҫса кайса ӳкрӗм.","ru":"Я поскользнулся и упал."}},{"id":"45","text":{"cv":"Ыйтма юрать-и?","ru":"Можно спросить?"}},{"id":"46","text":{"cv":"Ыранччен!","ru":"До завтра!"}},{"id":"47","text":{"cv":"Эпӗ кӗнеке вуласа тухрӑм.","ru":"Я прочитал книгу."}},{"id":"48","text":{"cv":"Эпӗ хирӗҫ мар.","ru":"Я не против."}},{"id":"49","text":{"cv":"Эпир печчӗн мар.","ru":"Мы не одни."}},{"id":"50","text":{"cv":"Эсӗ мӗн ятлӑ?","ru":"Как тебя зовут?"}},{"id":"51","text":{"cv":"Эсӗ миҫере?","ru":"Сколько тебе лет?"}},{"id":"52","text":{"cv":"Эсӗ пӗчченех килтӗн-и?","ru":"Ты пришел один?"}},{"id":"53","text":{"cv":"Яту мӗнле.","ru":"Как тебя зовут?"}}]